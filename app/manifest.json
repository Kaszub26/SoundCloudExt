{
    "name" : "DiscoveryCloud",
    "description" : "A music discovery stream powered by SoundCloud.",
    "permissions" : [
        "https://api.sndcdn.com/",
        "http://api.sndcdn.com/",
        "http://connect.soundcloud.com/",
        "http://*/"
    ],
    "background" : {
        "scripts" : ["background.js"]
    },
    "browser_action" : {
        "default_icon" : "icon48.png"
    },
    "icons" : {
        "16" : "icon16.png",
        "48" : "icon48.png",
        "128" : "icon128.png"
    },
    "manifest_version" : 2,
    "version" : "1.3",
    "update_url" : "http://cap1dev.conduit-apps.com/Apps/petar/soundcloud/updates.xml"
}